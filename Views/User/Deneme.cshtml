@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<KullaniciTarafi.Models.Kullanicilar>

<div class="row gy-4 isotope-container">
    @foreach (var item in Model)
    {
        <div class="col-lg-4 col-md-6">
            <div class="kullanici-item">
            <hi>@item.Ad</hi>
            <h2>@item.Soyad</h2>
            </div>
        </div>
    }
</div>